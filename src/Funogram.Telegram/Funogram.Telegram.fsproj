<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
    </PropertyGroup>
    <PropertyGroup>
      <VersionPrefix>6.3.0.0</VersionPrefix>
      <Authors>Nikolay Matyushin</Authors>
      <Product>Funogram.Telegram</Product>
      <Title>Funogram.Telegram</Title>
      <PackageId>Funogram.Telegram</PackageId>
      <PackageTags>telegram;funogram;F#;bot</PackageTags>
      <PackageIcon>logo.png</PackageIcon>
      <PackageProjectUrl>https://github.com/Dolfik1/Funogram</PackageProjectUrl>
      <PackageLicenseExpression>MIT</PackageLicenseExpression>
      <PublishRepositoryUrl>true</PublishRepositoryUrl> 
    </PropertyGroup>
    <ItemGroup>
        <Compile Include="Types.fs" />
        <Compile Include="RequestsTypes.fs" />
        <Compile Include="Api.fs" />
        <Compile Include="Sscanf.fs" />
        <Compile Include="Bot.fs" />
    </ItemGroup>
    <ItemGroup>
      <ProjectReference Include="..\Funogram\Funogram.fsproj" />
    </ItemGroup>
    <ItemGroup>
        <None Include="..\..\docs\files\img\logo.png" Pack="true" PackagePath="\"/>
    </ItemGroup>
</Project>
